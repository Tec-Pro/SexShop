	<parameter name="numFactura" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select
v.id as numVenta,v.monto as montoVenta,v.fecha,
c.id as id_clien ,c.nombre,c.apellido,c.telefono,c.celular,
pv.cantidad,
p.precio_venta,p.numero_producto,p.nombre as descr,p.tipo,p.marca
from
ventas v
inner join clientes c on v.idcliente=c.id
inner join productos_vendidos pv on v.id=pv.idventa
inner join productos p on pv.idproducto=p.numero_producto

where v.id=$P{numFactura}]]>
	</queryString>
